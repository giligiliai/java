//**************三元排序*******************************
int a1=3,a2=-4,a3=5
System.out.println(a1==a2?"a1=a2":(a1>a2?"a1>a2":"a2>a1"));
System.out.println(a1>a2?(a1>a3?(a2>a3?"a1>a2>a3":"a1>a3>a2"):"a3>a1>a2"):(a1>a3?"a2>a1>a3":(a3>a2?"a3>a2>a1":"a2>a3>a1")));
//*********************************************
